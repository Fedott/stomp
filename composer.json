{
    "name": "amphp/stomp",
    "homepage": "https://github.com/amphp/stomp",
    "description": "A non-blocking STOMP client built on the amp concurrency framework",
    "keywords": [
        "async",
        "asynchronous",
        "stomp",
        "message",
        "amp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net",
            "role": "Creator / Lead Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "amphp/amp": "dev-master",
        "amphp/socket": "dev-amp_v2",
        "amphp/loop": "dev-master",
        "amphp/stream": "dev-master as 0.1",
        "amphp/dns": "dev-amp_v2 as 0.2",
        "amphp/windows-registry": "dev-amp_v2 as 0.3",
        "amphp/process": "dev-amp_v2 as 0.2",
        "amphp/file": "dev-amp_v2 as 0.2",
        "amphp/cache": "dev-amp_v2 as 0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Stomp\\": "lib/"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Stomp\\Test\\": "test/"
        }
    },
    "support": {
        "issues": "https://github.com/amphp/stomp/issues"
    }
}
