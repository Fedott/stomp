{
    "name": "amphp/stomp",
    "homepage": "https://github.com/amphp/stomp",
    "description": "A non-blocking STOMP client built on the amp concurrency framework",
    "keywords": [
        "async",
        "asynchronous",
        "stomp",
        "message",
        "amp"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@php.net",
            "role": "Creator / Lead Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "amphp/amp": "^2",
        "amphp/socket": "^0.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "friendsofphp/php-cs-fixer": "^2"
    },
    "autoload": {
        "psr-4": {
            "Amp\\Stomp\\": "lib/"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Stomp\\Test\\": "test/"
        }
    },
    "support": {
        "issues": "https://github.com/amphp/stomp/issues"
    }
}
